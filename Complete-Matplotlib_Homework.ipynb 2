   "source": [
    "# Calculate the average fare per city:\n",
    "average_fare_per_city = df_pyber.groupby(['city']).mean()['fare']\n",
    "average_fare_df = average_fare_per_city.to_frame(name='Average Fare Per City')"
   ]
  },
  {
