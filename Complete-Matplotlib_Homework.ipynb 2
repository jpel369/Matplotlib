    "df_city = pd.read_csv('city_data.csv', index_col=['city'])\n",
    "df_ride = pd.read_csv('ride_data.csv', index_col=['city'])\n",
    "df_city_ride = df_city.merge(df_ride, on=['city'])\n",
    "df_pyber = df_city_ride.sort_values(['city'], ascending=True)\n",
    "df_pyber.head()"
   ]
  },
  {
